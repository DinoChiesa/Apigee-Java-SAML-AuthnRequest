<JavaCallout name='Java-SAML-AuthnRequest-4'>
  <Properties>
    <Property name='binding-type'>redirect</Property>
    <Property name='private-key'>{my_private_key}</Property>
    <Property name='certificate'>{my_certificate}</Property>
    <Property name='issuer'>{issuer}</Property>
    <Property name='acs-url'>{acs-url}</Property>
    <Property name='destination'>{destination}</Property>
    <Property name='service-provider-name'>TestServiceProvider</Property>
    <Property name='force-authn'>true</Property>
    <Property name='signature-method'>rsa-sha1</Property>
    <Property name='requester-id'>{requester-id}</Property>
    <Property name='relay-state'>{messageid}</Property>
    <Property name='url-encode-output'>false</Property>
  </Properties>
  <ClassName>com.google.apigee.edgecallouts.samlauthn.Generate</ClassName>
  <ResourceURL>java://apigee-samlauthn-20200720.jar</ResourceURL>
</JavaCallout>
